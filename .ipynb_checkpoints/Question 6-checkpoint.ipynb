    "from html.parser import HTMLParser\n",
    "\n",
    "class HeadingParser(HTMLParser):\n",
    "    def handle_data(self, data):\n",
    "        if '\\n' not in data:\n",
    "            print(data);\n",
    "\n",
    "htmlFileContent = open(\"w3c.txt\").read();\n",
    "\n",
    "parser = HeadingParser();\n",
    "parser.feed(htmlFileContent);\n",
    "\n"
   ]
